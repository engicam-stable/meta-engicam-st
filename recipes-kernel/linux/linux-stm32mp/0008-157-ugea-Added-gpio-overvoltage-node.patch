From: FraUtel <francesco.utel@engicam.com>
Date: Mon, 8 Jan 2024 15:26:06 +0100
Subject: [PATCH] 157 ugea: Added gpio overvoltage node

---
 arch/arm/boot/dts/stm32mp157a-ugea512-mx.dts | 10 ++++++++++
 1 file changed, 10 insertions(+)

diff --git a/arch/arm/boot/dts/stm32mp157a-ugea512-mx.dts b/arch/arm/boot/dts/stm32mp157a-ugea512-mx.dts
index 0f93c0b05..4ab5ebc28 100644
--- a/arch/arm/boot/dts/stm32mp157a-ugea512-mx.dts
+++ b/arch/arm/boot/dts/stm32mp157a-ugea512-mx.dts
@@ -265,6 +265,16 @@
 				 power-supply = <&panel_pwr>;
 
 	};
+	
+	gpio_export {
+		compatible = "gpio-export";
+		#size-cells = <0>;
+		gpio-overvoltage {
+			gpio-export,name = "GPIO-OVERVOLTAGE";
+			gpio-export,output = <0>;
+			gpios = <&gpiof 11 0>;
+		};
+	};
 	/* USER CODE END root */
 
 	clocks {
